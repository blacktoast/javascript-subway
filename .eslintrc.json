{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "ignorePatterns": ["cypress/"],
  "parser": "babel-eslint",
  "rules": {
    "no-new": "off",
    "no-alert": "off",
    "no-param-reassign": "off",
    "no-return-assign": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "max-depth": ["error", 2]
  }
}
